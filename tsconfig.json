{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@authumn/angular": [
        "dist/@authumn/angular"
      ],
      "@authumn/angular-templates-bootstrap3": [
        "dist/@authumn/angular-templates-bootstrap3"
      ],
      "@authumn/angular-templates-material": [
        "dist/@authumn/angular-templates-material"
      ],
      "@authumn/angular-user": [
        "dist/@authumn/angular-user"
      ],
      "@authumn/angular-auth": [
        "dist/@authumn/angular-auth"
      ]
    },
    "noUnusedLocals": false,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "es2017",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
